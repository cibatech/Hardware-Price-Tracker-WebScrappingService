
generator client {
  provider = "prisma-client-js"
  output = "indev-output"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL_SECONDARY")
}

model StaticLink{
  Id Int @id @default(autoincrement())
  Link String
  GeneratedAt DateTime @default(now())
  Where String
}

model Issue{
  Id String @id @default(uuid())
  When DateTime @default(now())
  Reason String? 
  At String?
}
